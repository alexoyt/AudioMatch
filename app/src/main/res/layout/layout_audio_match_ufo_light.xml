<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/audio_match_light"
        android:layout_width="195dp"
        android:layout_height="152dp"
        android:layout_marginTop="206dp"
        android:scaleType="centerInside"
        android:src="@drawable/icon_audio_match_light"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/audio_match_ufo"
        android:layout_width="112dp"
        android:layout_height="63.5dp"
        android:layout_marginLeft="67dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="167dp"
        android:src="@drawable/icon_audio_match_ufo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/audio_match_ripple_inside"
        android:layout_width="63.5dp"
        android:layout_height="38.5dp"
        android:src="@drawable/icon_audio_match_ripple_inside"
        app:layout_constraintTop_toTopOf="parent"
        android:rotation="17"
        android:layout_marginTop="298dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="3dp"
        android:layout_marginEnd="3dp"
        android:visibility="gone"
        />

    <ImageView
        android:id="@+id/audio_match_ripple_outside_1"
        android:layout_width="153dp"
        android:layout_height="66dp"
        android:src="@drawable/icon_audio_match_ripple_ouside"
        app:layout_constraintTop_toTopOf="parent"
        android:rotation="17"
        android:layout_marginTop="285dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="3dp"
        android:layout_marginEnd="3dp"
        android:visibility="gone"
        />

    <ImageView
        android:id="@+id/audio_match_ripple_outside_2"
        android:layout_width="153dp"
        android:layout_height="66dp"
        android:src="@drawable/icon_audio_match_ripple_ouside"
        app:layout_constraintTop_toTopOf="parent"
        android:rotation="17"
        android:layout_marginTop="285dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="3dp"
        android:layout_marginEnd="3dp"
        android:visibility="gone"
        />

    <ImageView
        android:id="@+id/audio_match_ripple_outside_3"
        android:layout_width="153dp"
        android:layout_height="66dp"
        android:src="@drawable/icon_audio_match_ripple_ouside"
        app:layout_constraintTop_toTopOf="parent"
        android:rotation="17"
        android:layout_marginTop="285dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="3dp"
        android:layout_marginEnd="3dp"
        android:visibility="gone"
        />

</android.support.constraint.ConstraintLayout>